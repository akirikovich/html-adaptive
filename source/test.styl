.page-header
	width 100%
	height 100px
	
	background-color #D7D7D7
	
.header-menu
	width 200px

	background-color blue

	.header-menu-item
		display block
		
.products
	.product-item
		width 48%
		height 150px
		
		img
			width auto
			height auto
			max-width 100%
			max-height 100%
			
.rates
	padding 20px 0

	span
		padding 0 10px
		
		display inline-block
		&:first-child
			padding-left 0
			
		&:last-child
			padding-right 0